<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>

<AnchorPane fx:id="tutorial" prefHeight="600.0" prefWidth="800.0" styleClass="background" stylesheets="@../css/Tutorial.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TutorialController">
   <children>
      <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="610.0" prefWidth="800.0" spacing="5.0" styleClass="background" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <MediaView fx:id="mvMedia" fitHeight="400.0" fitWidth="500.0" />
            <Slider fx:id="sliderMedia" />
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#handlerNegative" prefHeight="50.0" prefWidth="100.0" text="-" />
                  <Button mnemonicParsing="false" onAction="#handlerPlay" prefHeight="50.0" prefWidth="100.0" text="Play" />
                  <Button mnemonicParsing="false" onAction="#handlerPause" prefHeight="50.0" prefWidth="100.0" text="Pause" />
                  <Button mnemonicParsing="false" onAction="#handlerPositive" prefHeight="50.0" prefWidth="100.0" text="+" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
